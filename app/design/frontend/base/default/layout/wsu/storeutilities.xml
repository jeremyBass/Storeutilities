<?xml version="1.0"?>
<layout version="0.1.0">
    <jQuery>
		<reference name="head">
			<action method="addItem" ifconfig="storeutilities_conf/general_settings/fejqueryuicss">
				<type>cdn_css</type>
				<name>//code.jquery.com/ui/1.10.4/themes/smoothness/jquery-ui.min.css</name>
				<params/>
				<if/>
				<cond/>
				<window_obj>jQuery</window_obj>
				<local_path>/js/jquery-ui.smoothness.min.1.10.4.css</local_path>
			</action>
			<action method="addItem">
				<type>cdn_css</type>
				<name>//netdna.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css</name>
			</action>
			<action method="addItem" ifconfig="storeutilities_conf/general_settings/fejquery">
				<type>cdn_js</type>
				<name>//code.jquery.com/jquery-1.11.1.min.js</name>
				<params/>
				<if/>
				<cond/>
				<window_obj>jQuery</window_obj>
				<local_path>/js/jquery-1.11.1.min.js</local_path>
			</action>
			<action method="addItem" ifconfig="storeutilities_conf/general_settings/fejqueryui">
				<type>cdn_js</type>
				<name>//code.jquery.com/ui/1.10.4/jquery-ui.min.js</name>
				<params/>
				<if/>
				<cond/>
				<window_obj>jQuery</window_obj>
				<local_path>/js/jquery-ui.min.1.10.4.js</local_path>
			</action>
			<action method="addItem" ifconfig="storeutilities_conf/general_settings/fejquerymigrate">
				<type>cdn_js</type>
				<name>//code.jquery.com/jquery-migrate-1.2.1.min.js</name>
				<params/>
				<if/>
				<cond/>
				<window_obj>jQuery</window_obj>
				<local_path>/js/jquery-migrate-1.2.1.min.js</local_path>
			</action>
			<action method="addItem" ifconfig="storeutilities_conf/general_settings/fejquery">
				<type>cdn_js</type>
				<name>/js/noConflict.js</name>
			</action>

			
		</reference>
    </jQuery>
    <default translate="label" module="page">
		<update handle="jQuery"/>
    </default>
</layout>
